---
export function getStaticPaths() {
  return [
    {params: {title: 'pirveli-grdzeli'}, props: {info: longTermServices[0]}},
    {params: {title: 'meore-grdzeli'}, props: {info: longTermServices[1]}},
    {params: {title: 'pirveli-erteuli'}, props: {info: oneOffServices[0]}},
    {params: {title: 'meore-erteuli'}, props: {info: oneOffServices[1]}},
  ];
}

import { Container, TextSection } from '@components/odyssey-theme';
import Layout from '../../../layouts/Page.astro';
import { longTermServices, oneOffServices } from './services';


const { info } = Astro.props;
---

<Layout>
    <Container>
        <TextSection>
            <h1>{info.title}</h1>
        </TextSection>
    </Container>
</Layout>
